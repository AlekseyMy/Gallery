<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.alekseimartoyas.webgallery">

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_insert_photo_black_24dp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_insert_photo_black_24dp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Modules.PicturesShowing.view.PicturesShowingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Modules.PictureDetailed.view.PictureDetailedActivity">
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>